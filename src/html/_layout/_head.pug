  var titleMerge = page ? ((page.title != '') ? page.title + ' â”‚ ' + data.meta.websiteName : data.meta.websiteName ) : "missing";
  var keywordsMerge = page ? ((page.keywords != '') ? page.keywords + ', ' + data.meta.keywords : metaKeywordsBase) : "missing";
  var pageTescription = page ? page.description : "missing";
  var pageId = page ? page.id : "missing";
  var pageTitle = page ? page.title : "missing";
